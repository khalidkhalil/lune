<div id="<%= dom_id movie %>">
  <p>
    <strong>Name:</strong>
    <%= movie.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= movie.description %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= movie.year %>
  </p>

  <p>
    <strong>Director:</strong>
    <%= movie.director %>
  </p>

  <p>
    <strong>Actor:</strong>
    <%= movie.actor %>
  </p>

  <p>
    <strong>Filming location:</strong>
    <%= movie.filming_location %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= movie.country %>
  </p>

</div>
